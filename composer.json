{
    "name": "rokka/client",
    "type": "library",
    "description": "Rokka.io client library",
    "keywords": [
      "rokka",
      "rokka.io",
      "image",
      "images",
      "imagemanipulation"
    ],
    "homepage": "https://github.com/rokka-io/rokka-client-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Riesen",
            "email": "chris.riesen@gmail.com",
            "homepage": "http://christianriesen.com",
            "role": "Developer"
        },
        {
            "name": "Matteo De Micheli",
            "email": "mat.demicheli@gmail.com",
            "homepage": "http://matteodem.ch",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.0",
        "jbroadway/urlify": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.27",
        "symfony/var-dumper": "^3.4|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Rokka\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rokka\\Client\\Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/var-dumper/Resources/functions/dump.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
